{
  "expo": {
    "name": "banco-riku",
    "slug": "banco-riku",
    "extra": {
      "EXPO_PUBLIC_STAGE": "prod",
      "EXPO_PUBLIC_API_URL": "https://banco-riku-backend-express-js.onrender.com/api",
      "EXPO_PUBLIC_API_URL_IOS": "https://banco-riku-backend-express-js.onrender.com/api",
      "EXPO_PUBLIC_API_URL_ANDROID": "https://banco-riku-backend-express-js.onrender.com/api",
      "router": {},
      "eas": {
        "projectId": "da5c7c47-a89d-4850-a292-e57623528eda"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo_banco_riku.png",
    "scheme": "bancoriku",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.bancoriku"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Logo_banco_riku.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo_banco_riku.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
